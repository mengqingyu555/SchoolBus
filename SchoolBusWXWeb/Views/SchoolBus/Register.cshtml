@{
    Layout = "~/Views/Shared/_SchoolBusLayout.cshtml";
    ViewBag.Title = "注册";
}
@using SchoolBusWXWeb.Models.PmsData
@model RegisterModel
@section Css{
    <link href="~/lib/mui/css/mui.picker.min.css" rel="stylesheet" />
    <style>
        .mui-checkbox input[type=checkbox]:checked:before, .mui-radio input[type=radio]:checked:before {
            color: #FCC142 !important;
        }

        .mui-checkbox input[type=checkbox]:before, .mui-radio input[type=radio]:before {
            font-size: 24px;
        }

        .mui-checkbox.mui-left input[type=checkbox], .mui-radio.mui-left input[type=radio] {
            left: 13px;
        }
    </style>
}
<div class="register_pic">
    @ViewData["OpenId"]
    <img src="~/img/pic1.jpg" />
</div>
<div class="register_login">
    <form id="frm" name="frm" method="post" asp-action="Register" autocomplete="off" asp-antiforgery="true">
        <div class="login_content">
            <div class="input">
                <img src="~/img/login-icon1.png" />
                <div class="input-wrapper" onclick="$(this).find('input').focus()">
                    <span style="width: 10%">卡号</span>
                    @*3603631297*@
                    <input type="text" asp-for="cardNum" value="@(Model?.cardNum)" maxlength="10" class="input" style="width: 50%; float: right" />
                    <span asp-validation-for="cardNum" style="width: 40%;color: red;"></span>
                </div>
            </div>
            <div class="input">
                <img src="~/img/login-icon4.png" />
                <div class="input-wrapper" id="relationshipname">
                    <span style="width: 10%">与学生关系</span>
                    <!--placeholder="选择与学生的关系">-->
                    <input type="text" asp-for="relationship" value="@(Model?.relationship)" class="input" style="width: 20%; float: right" readonly="readonly"/>
                    <span asp-validation-for="relationship" style="width: 60%;color: red"></span>
                </div>
            </div>
            <div class="input">
                <img src="~/img/login-icon2.png" />
                <div class="input-wrapper" onclick="$(this).find('input').focus()">
                    <span style="width: 1%">手机号码</span>
                    <input type="text" asp-for="phoneNum" value="@(Model?.phoneNum)" maxlength="11" class="input" style="width: 45%; float: right" />
                    <span asp-validation-for="phoneNum" style="width: 54%;color: red"></span>
                </div>
            </div>
            <div class="input">
                <img src="~/img/login-icon3.png" />
                <div class="input-wrapper" onclick="$(this).find('input').focus()">
                    <span style="width: 20%">验证码</span>
                    <input type="text" asp-for="verificationCode" maxlength="6" class="input" style="text-align: right; width: 40%; float: right" />
                    <span asp-validation-for="verificationCode" style="width: 40%;color: red"></span>
                </div>
            </div>
            <div style="border: none" class="input">
                <div class="input-wrapper" onclick="$(this).find('input').focus()">
                    <button style="width: 45%; float: right" id="generateCode" onclick="sendSmsCode(0)" type="button" class="check_code">
                        获取验证码
                    </button>
                </div>
            </div>
            <div class="agree mui-input-row mui-checkbox mui-left">
                <input id="checkbox" name="checkbox" type="checkbox" checked="checked">
                <p> 我已经同意<a onclick="popoverClick('#div')" href="javascript:;"> 服务条款</a></p>
            </div>
            <button type="submit" class="btn mui-btn mui-btn-warning">马上注册</button>
        </div>
    </form>
</div>
